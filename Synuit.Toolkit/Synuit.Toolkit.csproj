    <RepositoryType>C#</RepositoryType>
    <PackageProjectUrl>https://github.com/Synuit/Synuit.Toolkit</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Synuit/Synuit.Toolkit</RepositoryUrl>
    <PackageIconUrl>http://www.javelinmediagroup.com/wp-content/uploads/2018/04/synuit_packages.png</PackageIconUrl>
    <PackageReleaseNotes>v1.0.6 Modified to target .NET Standard 2.1 to enhance cross-platform capabilities.</PackageReleaseNotes>
